{
  "name": "eshop-price-telegram-bot",
  "version": "1.0.0",
  "description": "Telegram bot for Nintendo eShop price comparison across regions",
  "main": "src/bot.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node src/bot.js",
    "dev": "nodemon src/bot.js",
    "pm2:start": "pm2 start src/bot.js --name eshop-bot",
    "pm2:stop": "pm2 stop eshop-bot",
    "pm2:restart": "pm2 restart eshop-bot",
    "pm2:logs": "pm2 logs eshop-bot"
  },
  "dependencies": {
    "node-telegram-bot-api": "^0.64.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": ["telegram", "bot", "nintendo", "eshop", "price", "comparison"],
  "author": "",
  "license": "MIT"
}